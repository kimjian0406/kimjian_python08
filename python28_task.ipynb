    {
      "cell_type": "code",
      "source": [
        "def min_max(*numbers):\n",
        "    numbers_tuple = tuple(numbers)\n",
        "    print(\"최소값:\", min(numbers_tuple), \"최대값:\", max(numbers_tuple))\n",
        "\n",
        "min_max(15, 7, 5)"
